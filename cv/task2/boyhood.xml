<?xml version="1.0" ?>
<!-- config file for the 'boyhood' dataset -->
<config>
	
	<test_svm>0</test_svm>
	<test_hog>0</test_hog>
	<test_bonus>0</test_bonus>
	<output_dir>./output/boyhood/</output_dir>
	<test_svm_out>./output/boyhood/svm.png</test_svm_out>
	<test_hog_out>./output/boyhood/hog.png</test_hog_out>
	<test_bonus_out>./output/boyhood/dt.png</test_bonus_out>

  <!------------------------
          Input data
    ----------------------->
  
	<in_face_filename>./data/boyhood/one.png</in_face_filename>
	<in_face_filename>./data/boyhood/two.png</in_face_filename>
	<in_face_filename>./data/boyhood/three.png</in_face_filename>
	<in_face_filename>./data/boyhood/four.png</in_face_filename>

  <!-------------------------------
          SVM parameters
    ------------------------------>
    <svm_training>
		<dir>data/training_data/</dir>
		<img>
			<scale>0.75f</scale>
			<number>60</number>
		</img>
    </svm_training>

	<!-------------------------------
          Detection parameters
    ------------------------------>
    <best_score_num>30</best_score_num>

  <!-------------------------------
          Algorithm parameters
    ------------------------------>

  <s1>1.8f</s1>
  <s2>1.4f</s2>
  <transform_time>6000</transform_time>
  <fps>30</fps>

  <!------------------------
          Output data
    -----------------------> 
    
	<out_face_dedect_filename>./output/boyhood/face_detect_boyhood1.png</out_face_dedect_filename>
        <out_face_dedect_filename>./output/boyhood/face_detect_boyhood2.png</out_face_dedect_filename>
        <out_face_dedect_filename>./output/boyhood/face_detect_boyhood3.png</out_face_dedect_filename>
        <out_face_dedect_filename>./output/boyhood/face_detect_boyhood4.png</out_face_dedect_filename>
        
	<out_face_mask_filename>./output/boyhood/face_mask_boyhood1.png</out_face_mask_filename>
        <out_face_mask_filename>./output/boyhood/face_mask_boyhood2.png</out_face_mask_filename>
        <out_face_mask_filename>./output/boyhood/face_mask_boyhood3.png</out_face_mask_filename>
        <out_face_mask_filename>./output/boyhood/face_mask_boyhood4.png</out_face_mask_filename>
        
	<out_face_trans_filename>./output/boyhood/face_trans_boyhood1.png</out_face_trans_filename>
        <out_face_trans_filename>./output/boyhood/face_trans_boyhood2.png</out_face_trans_filename>
        <out_face_trans_filename>./output/boyhood/face_trans_boyhood3.png</out_face_trans_filename>
        <out_face_trans_filename>./output/boyhood/face_trans_boyhood4.png</out_face_trans_filename>
        
	<out_face_trimmed_filename>./output/boyhood/face_trimmed_boyhood1.png</out_face_trimmed_filename>
        <out_face_trimmed_filename>./output/boyhood/face_trimmed_boyhood2.png</out_face_trimmed_filename>
        <out_face_trimmed_filename>./output/boyhood/face_trimmed_boyhood3.png</out_face_trimmed_filename>
        <out_face_trimmed_filename>./output/boyhood/face_trimmed_boyhood4.png</out_face_trimmed_filename>
        
        <detect_face>./output/boyhood/boyhood_detect_face.png</detect_face>
        <face_sequence_filename>./output/boyhood/boyhood_face_sequence.png</face_sequence_filename>	
        <outliers>./output/boyhood//boyhood_outliers.png</outliers>
        <video_out>./output/boyhood/boyhood.avi</video_out>
</config>
